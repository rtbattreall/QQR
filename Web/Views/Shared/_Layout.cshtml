    <div class="header-wrapper">
        <section>
            <div class="toolbar">
                <div class="container">
                    <div class="sixteen columns">
                        <div class="phone">Phone: <span class="white">402.895.5066</span> <span class="time">(8am - 5pm CST Mon-Fri)</span></div>
                        <!--<div class="login">Welcome, <span class="white">Guest</span> <a href="javascript:void(0);">Login</a></div>-->
                    </div>
                </div>
            </div>
        </section>
        <header>
            <div class="container">
                <div class="sixteen columns">
                    <div class="white-burst">
                        <div class="logo">
                            <a href="javascript:void(0);">
                                <h1>Quinn's Reporting</h1>
                            </a>
                        </div>
                    </div>
                    @RenderSection("Navigation")
                </div>
            </div>
        </header>
        <!-- .end header-wrapper -->
    </div>

    @RenderBody()

    @Html.Partial("_Footer");
    
    @Scripts.Render("~/Scripts/jquery/jquery-1.9.0.min.js")
    @Scripts.Render("~/Scripts/jquery/jquery-ui-1.10.0.min.js")
    <script>window.jQuery || document.write('<script src="Scripts/jquery/jquery-1.9.0.min.js"><\/script>')</script>
    @Scripts.Render("~/Scripts/jquery/jquery.validate.min.js")
    @Scripts.Render("~/Scripts/jquery/jquery.validate.unobtrusive.min.js")
    @Scripts.Render("~/bundles/js")
    @RenderSection("scripts", required: false)

