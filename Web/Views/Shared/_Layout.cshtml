    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

    @Styles.Render("~/Content/Custom/css")
    @Scripts.Render("~/Scripts/jquery/modernizr-2.6.2.min.js")

</head>
<body>
    <!--[if lt IE 7]>
